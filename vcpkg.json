{
  "name": "druid",
  "version-string": "2025.10.9",
  "port-version": 1,
  "description": "Qt based C++ application, simulation, and game engine",
  "homepage": "https://gitlab.com/druidengine/druid",
  "dependencies": [
    {
      "name": "qtbase",
      "default-features": false,
      "host": true
    },
    {
      "name": "qtbase",
      "default-features": false,
      "features": [
        "png",
        "opengl"
      ]
    },
    {
      "name": "qtbase",
      "default-features": false,
      "features": [
        "egl",
        "fontconfig",
        "xcb",
        "xcb-xlib",
        "xrender"
      ],
      "platform": "linux"
    },
    {
      "name": "qtbase",
      "default-features": false,
      "features": [
        "egl",
        "gles2"
      ],
      "platform": "android"
    },
    {
      "name": "qtbase",
      "default-features": false,
      "features": [
        "gles2"
      ],
      "platform": "emscripten"
    },
    "qtdeclarative",
    "gtest"
  ]
}